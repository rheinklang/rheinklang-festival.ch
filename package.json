{
  "name": "website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "flexboxgrid": "6.3.1",
    "node-sass-chokidar": "1.3.0",
    "react": "16.8.6",
    "react-countdown-now": "1.3.0",
    "react-dom": "16.8.6",
    "react-scripts-ts": "3.1.0"
  },
  "scripts": {
